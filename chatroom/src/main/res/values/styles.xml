<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="dialog_title_style">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">39dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/textColor</item>
        <item name="android:textSize">15dp</item>
    </style>

    <style name="default_chebox">
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/fg_mannger_btn</item>
        <item name="android:textSize">11sp</item>
        <item name="android:button">@null</item>
        <item name="android:enabled">false</item>
        <item name="android:background">@drawable/checked_mannger_btn</item>
    </style>

    <style name="live_friend_radio_style">
        <item name="android:layout_width">80dp</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:textColor">@color/fg_live_friend_radio_color</item>
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">10dp</item>
        <item name="android:enabled">false</item>
        <item name="android:background">@drawable/checked_bg_live_friend_radio</item>
    </style>

    <style name="live_friend_radio_button_style">
        <item name="android:layout_width">60dp</item>
        <item name="android:layout_height">22dp</item>
        <item name="android:background">@drawable/bg_al_50_black_radius_10</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">9dp</item>
        <item name="android:button">@null</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:paddingLeft">3dp</item>
        <item name="android:paddingRight">3dp</item>
        <item name="android:alpha">0.5</item>
    </style>

    <style name="live_label_style">
        <item name="android:layout_width">36dp</item>
        <item name="android:layout_height">18dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">11dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textStyle">bold</item>
    </style>





</resources>